<Window x:Class="WpfAppKalkulator4a.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppKalkulator4a"
        mc:Ignorable="d"
        Title="Kalkulator pomiędzy systemami" Height="250" Width="400">
    <UniformGrid Columns="2">
        <TextBlock>Podaj liczbę</TextBlock>
        <TextBox x:Name="liczbaTextBox"></TextBox>
        <TextBlock>Podaj system</TextBlock>
        <ComboBox x:Name="systemCombobox" SelectedIndex="0"></ComboBox>
        <Button Click="Button_Click_ZmienNaDziesietne">Zamień na dziesiętne</Button>
        <Button Click="Button_Click_Zamien_NaSystem">Zamień na system</Button>
    </UniformGrid>
</Window>
